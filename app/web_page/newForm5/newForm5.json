      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.okr ){",
        "\treturn {",
        "\t\t//map okr variable to expected task contract input",
        "\t\tokrInput: {",
        "\t\t\tpersistenceId_string: $data.okr.persistenceId_string !== undefined ? $data.okr.persistenceId_string : null,",
        "\t\t\tinputString: $data.okr.inputString !== undefined ? $data.okr.inputString : null,",
        "\t\t\toutputString: $data.okr.outputString !== undefined ? $data.okr.outputString : null,",
        "\t\t\toutcomeString: $data.okr.outcomeString !== undefined ? $data.okr.outcomeString : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
