    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.okr ){",
        "\treturn {",
        "\t\t//map okr variable to expected task contract input",
        "\t\tokrInput: $data.okr.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\tinputString: it.inputString !== undefined ? it.inputString : null,",
        "\t\t\toutputString: it.outputString !== undefined ? it.outputString : null,",
        "\t\t\toutcomeString: it.outcomeString !== undefined ? it.outcomeString : null",
        "\t\t}))",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "okr" : {
      "type" : "url",
      "value" : [
