    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.okr ){",
        "\treturn {",
        "\t\t//map okr variable to expected task contract input",
        "\t\tokrInput: $data.okr.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
