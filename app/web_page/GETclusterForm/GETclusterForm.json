    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.clusterCollection && $data.clusterCollection_clusterCollectionContent ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.clusterCollection.clusterCollectionContent = $data.clusterCollection_clusterCollectionContent;",
        "\treturn {",
        "\t\t//map clusterCollection variable to expected task contract input",
        "\t\tclusterCollectionInput: {",
        "\t\t\tclusterCollectionContent: $data.clusterCollection.clusterCollectionContent.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tclusterContent: it.clusterContent !== undefined ? it.clusterContent : null,",
        "\t\t\t\tclusterTitle: it.clusterTitle !== undefined ? it.clusterTitle : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
